set(USE_SSH ON CACHE BOOL "ssh" FORCE)
set(USE_HTTPS ON CACHE BOOL "ssl" FORCE)
set(BUILD_CLAR OFF CACHE BOOL "no test" FORCE)
set(BUILD_TESTS OFF CACHE BOOL "no test" FORCE)
set(BUILD_SHARED_LIBS OFF CACHE BOOL "link static" FORCE)
set(BUILD_CLI OFF CACHE BOOL "BUILD_CLI" FORCE)
set(USE_BUNDLED_ZLIB OFF CACHE BOOL "Force using bundled zlib" FORCE)
set(USE_NTLMCLIENT OFF CACHE BOOL "NTLM requires a newer openssl API than the boringssl one we use" FORCE)
set(USE_GSSAPI OFF CACHE BOOL "The recent changes in the GSSAPI support clash with our use of openssl instead of Winhttp on Windows" FORCE)